{
    "name": "nestjs-authentication",
    "version": "0.1.7",
    "description": "A nestjs module that support multiple authentications provider",
    "author": "Alex Hermann <contact@pop-code.com>",
    "repository": "https://github.com/Pop-Code/nestjs-authentication.git",
    "license": "MIT",
    "main": "dist/index.js",
    "types": "./dist/index.d.ts",
    "scripts": {
        "build": "rm -Rf dist && tsc -b",
        "doc": "rm -Rf ./docs && typedoc ./src && touch ./docs/.nojekyll",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest",
        "lint": "eslint --ext .js,.ts,.jsx,.tsx .",
        "format": "prettier \"**/*.ts\" --ignore-path .prettierignore --write && git status"
    },
    "peerDependencies": {
        "@nestjs/common": "^7",
        "@nestjs/core": "^7",
        "@nestjs/passport": "^7",
        "nestjs-console": "^4.0.0",
        "passport": "^0.4.1"
    },
    "dependencies": {
        "passport-local": "~1.0.0"
    },
    "devDependencies": {
        "@nestjs/common": "7.5.5",
        "@nestjs/core": "7.5.5",
        "@nestjs/testing": "7.5.5",
        "@nestjs/passport": "7.1.5",
        "@types/jest": "26.0.15",
        "@types/node": "14.14.10",
        "@types/passport": "1.0.4",
        "commander": "6.2.0",
        "@typescript-eslint/eslint-plugin": "4.21.0",
        "@typescript-eslint/parser": "4.8.2",
        "eslint": "7.14.0",
        "eslint-config-prettier": "6.15.0",
        "eslint-config-standard-with-typescript": "19.0.1",
        "eslint-plugin-import": "2.22.1",
        "eslint-plugin-node": "11.1.0",
        "eslint-plugin-prefer-arrow": "1.2.2",
        "eslint-plugin-promise": "4.2.1",
        "eslint-plugin-standard": "5.0.0",
        "jest": "26.6.3",
        "nestjs-console": "4.0.0",
        "passport": "0.4.1",
        "prettier": "2.2.1",
        "reflect-metadata": "0.1.13",
        "rxjs": "6.6.3",
        "ts-jest": "26.4.4",
        "ts-node": "9.0.0",
        "tsconfig-paths": "3.9.0",
        "typedoc": "0.19.2",
        "typescript": "4.0.5"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".spec.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.{js,jsx,ts}",
            "!index.ts",
            "!**/test/**"
        ],
        "coverageDirectory": "../coverage"
    },
    "engines": {
        "node": ">=0.12"
    }
}

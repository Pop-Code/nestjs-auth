{
    "name": "nestjs-authentication",
    "version": "0.0.7",
    "description": "A nestjs module that support multiple authentications provider",
    "author": "Alex Hermann <contact@pop-code.com>",
    "repository": "https://github.com/Pop-Code/nestjs-authentication.git",
    "license": "MIT",
    "main": "lib/index.js",
    "scripts": {
        "build": "rm -Rf lib && tsc -b",
        "format": "prettier --write \"src/**/*.{ts,tsx}\" --parser typescript",
        "doc": "rm -Rf ./docs && typedoc ./src && touch ./docs/.nojekyll",
        "test": "jest",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "lint": "eslint src/**/*"
    },
    "peerDependencies": {
        "@nestjs/common": "^6",
        "@nestjs/core": "^6",
        "@nestjs/passport": "^6.1.1",
        "nestjs-console": "^2.0.1",
        "passport": "^0.4.1"
    },
    "dependencies": {
        "passport-local": "~1.0.0"
    },
    "devDependencies": {
        "@nestjs/common": "6.10.14",
        "@nestjs/core": "6.10.14",
        "@nestjs/passport": "6.2.0",
        "@nestjs/testing": "6.10.14",
        "@types/jest": "24.9.0",
        "@types/node": "13.1.7",
        "@types/passport": "1.0.2",
        "@typescript-eslint/eslint-plugin": "2.16.0",
        "@typescript-eslint/eslint-plugin-tslint": "^2.17.0",
        "@typescript-eslint/parser": "2.16.0",
        "commander": "4.1.0",
        "eslint": "6.8.0",
        "eslint-plugin-import": "2.20.0",
        "eslint-plugin-prefer-arrow": "^1.1.7",
        "jest": "24.9.0",
        "nestjs-console": "2.0.1",
        "passport": "0.4.1",
        "prettier": "1.19.1",
        "reflect-metadata": "0.1.13",
        "rxjs": "6.5.4",
        "ts-jest": "24.3.0",
        "ts-node": "8.6.2",
        "tsconfig-paths": "3.9.0",
        "typedoc": "0.16.6",
        "typescript": "3.7.4"
    }
}
